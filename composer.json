{
    "require": {
        "php": "^7.2",
        "robmorgan/phinx": "0.12.1",
        "symfony/yaml": "^5.0",
        "tecnick.com/tcpdf": "^6.3",
        "dompdf/dompdf": "^0.8.5",
        "phpstan/phpstan": "~0.12.86",
        "aws/aws-sdk-php": "^3.185",
        "phpoffice/phpword": "v0.18.*",
        "guzzlehttp/guzzle": "^6.2",
        "ncjoes/office-converter": "^1.0",
        "convertapi/convertapi-php": "^1.5",
        "mnvx/lowrapper": "^1.0"
    },
    "autoload": {
        "psr-4":{
            "Custom\\": "framework/Custom/src",
            "Application\\" : "module/Application/src",
            "Domain\\" : "module/Domain/src",
            "Utility\\" : "module/Utility/src",
            "Event\\" : "module/Event/src",
            "Core\\" : "module/Core/src",
            "Test\\" : "module/Test/src",
            "Student\\" : "module/Student/src",
            "Employee\\" : "module/Employee/src"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
         "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phploc/phploc": "^4.0",
        "squizlabs/php_codesniffer": "^3.4",
        "sebastian/phpcpd": "^4.1",
        "phpunit/phpunit": "^7.5"
    }
}
